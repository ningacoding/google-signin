{
  "name": "@react-native-google-signin/google-signin",
  "version": "12.1.0",
  "description": "Google sign in for your react native applications",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "expo",
    "RNGoogleSignin.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "app.plugin.js",
    "expo-module.config.json",
    "plugin/build",
    "jest/build/setup.js",
    "README.md"
  ],
  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "nuke-ios": "rm -rf example/ios/Podfile.lock && rm -rf example/ios/Pods && rm -rf example/ios/build",
    "build:mock": "tsc --build jest && cp jest/build/jest/setup.js jest/build/setup.js",
    "build:plugin": "tsc --build plugin",
    "clean:plugin": "expo-module clean plugin",
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build && yarn build:plugin && yarn build:mock",
    "release": "yarn prepare && npx semantic-release",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "prettier:check": "prettier --list-different '**/*.{js,ts,tsx}' '**/*.md'",
    "prettier:write": "prettier --write '**/*.{js,ts,tsx}' '**/*.md'"
  },
  "keywords": [
    "react-native",
    "google signin",
    "google login",
    "oauth",
    "oauth2",
    "ios",
    "android",
    "web"
  ],
  "repository": "https://github.com/react-native-google-signin/google-signin-next",
  "author": "Vojtech Novak <vonovak@gmail.com> (https://github.com/vonovak)",
  "bugs": {
    "url": "https://github.com/react-native-google-signin/google-signin/issues"
  },
  "homepage": "https://github.com/react-native-google-signin/google-signin#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@commitlint/config-conventional": "^13.2.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.5",
    "@semantic-release/git": "^10.0.1",
    "@types/google-one-tap": "^1.2.5",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.14",
    "commitlint": "^13.2.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "expo": "^47.0.0",
    "expo-module-scripts": "^3.0.7",
    "husky": "^4.2.5",
    "jest": "^29.5.0",
    "patch-package": "^7.0.0",
    "pod-install": "^0.1.38",
    "prettier": "^3.1.0",
    "react": "18.2.0",
    "react-native": "^0.72.7",
    "react-native-builder-bob": "^0.23.1",
    "react-native-test-app": "^2.5.8",
    "semantic-release": "^19.0.5",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "expo": ">=47.0.0",
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "resolutions": {
    "@babel/traverse": "7.22.8",
    "@babel/core": "7.22.8"
  },
  "codegenConfig": {
    "name": "RNGoogleSignInCGen",
    "type": "all",
    "jsSrcsDir": "src/spec",
    "android": {
      "javaPackageName": "com.reactnativegooglesignin"
    }
  },
  "dependencies": {
    "jwt-decode": "^4.0.0"
  }
}
